{
  "name": "oro-functions-client",
  "version": "2.0.1",
  "description": "Ofn contains utility static methods (helpers): General, Numbers, Strings, Functions, Objects, Arrays, Dates, URLs, Files, PHP Serialize, and Response.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean"
  },
  "keywords": [
    "constant",
    "functions",
    "helpers",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-functions-client.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-functions-client/issues"
  },
  "homepage": "https://github.com/oropesa/oro-functions-client#readme",
  "dependencies": {
    "crypto-js": "^4.1.1",
    "js-base64": "^3.7.5",
    "lodash.clonedeep": "^4.5.0",
    "oro-php-locutus": "^2.0.1",
    "oro-regexp": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-typescript": "^7.23.2",
    "@types/crypto-js": "^4.1.3",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/lodash.clonedeep": "^4.5.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.52.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-unicorn": "^48.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  }
}
