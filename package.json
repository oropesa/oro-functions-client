{
  "name": "oro-functions-client",
  "version": "2.2.0",
  "description": "Ofn contains utility static methods (helpers): General, Numbers, Strings, Functions, Objects, Arrays, Dates, URLs, Files, PHP Serialize, and Response.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "coverage:open": "node coverage.open.js",
    "coverage-open": "jest --coverage & node coverage.open.js",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "clean": "rm -R ./dist",
    "clean:all": "rm -R ./dist ./node_modules",
    "build": "tsup ./src/index.ts",
    "watch": "nodemon"
  },
  "keywords": [
    "constant",
    "functions",
    "helpers",
    "typescript"
  ],
  "author": "Carlos Oropesa <carlos@oropensando.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oropesa/oro-functions-client.git"
  },
  "bugs": {
    "url": "https://github.com/oropesa/oro-functions-client/issues"
  },
  "homepage": "https://github.com/oropesa/oro-functions-client#readme",
  "dependencies": {
    "crypto-js": "^4.2.0",
    "js-base64": "^3.7.7",
    "lodash.clonedeep": "^4.5.0",
    "oro-php-locutus": "^2.0.3",
    "oro-regexp": "^2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash.clonedeep": "^4.5.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-alloy": "^5.1.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-unicorn": "^51.0.1",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  }
}
